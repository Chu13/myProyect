<h2> Search Results </h2>


<ul>
   <% listOfResults.forEach((onePlace) => { %>
     <li>
       <h3> <%= onePlace.name %> </h3>
       <p> <%=onePlace.country %> </p>
       <img src="<%=onePlace.image %>" />
     </li>
     <li>

       <a href="/places/details/<%=onePlace._id%>">
          View Details
       </a>
     </li>
   <% }) %>
</ul>
